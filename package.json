{
  "name": "bearing",
  "type": "module",
  "version": "0.6.1",
  "description": "A carousel component",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c --configPlugin typescript",
    "format": "prettier --write \"{**/*,*}\"",
    "format-check": "prettier --check \"{**/*,*}\"",
    "lint": "yarn run lint-check --fix",
    "lint-check": "eslint \"{**/*,*}.{ts,tsx}\"",
    "style": "yarn run style-check --fix",
    "style-check": "stylelint \"{**/*,*}.{scss,sass}\"",
    "start": "rollup -c -w --configPlugin typescript",
    "test": "mocha --reporter min 'test/**'",
    "coverage": "c8 yarn run test",
    "all": "yarn run build && yarn run test",
    "yalc": "yalc publish --sig --push"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "mocha": "^10.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.17.2",
    "rollup-plugin-postcss-modules": "^2.1.1",
    "sass": "^1.58.3",
    "source-map-support": "^0.5.21",
    "stylelint": "^15.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "easing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosingrind/bearing.git"
  },
  "keywords": [
    "carousel",
    "gallery",
    "slideshow",
    "react"
  ],
  "author": {
    "name": "Adam Barton",
    "email": "<108860307+rosingrind@users.noreply.github.com>"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rosingrind/bearing/issues"
  },
  "homepage": "https://rosingrind.github.io/bearing-home/",
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
